import kotlin.Boolean;
import kotlinx.datetime.Instant;

CREATE TABLE IF NOT EXISTS `bookmarks` (
    `id` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    `externalId` INTEGER AS kotlin.Long NOT NULL,
    `url` TEXT,
    `title` TEXT,
    `description` TEXT,
    `archived` INTEGER AS kotlin.Boolean NOT NULL,
    `unread` INTEGER AS kotlin.Boolean NOT NULL,
    `added` TEXT AS kotlinx.datetime.Instant NOT NULL,
    `modified` TEXT AS kotlinx.datetime.Instant NOT NULL,
    `tags` TEXT
);
